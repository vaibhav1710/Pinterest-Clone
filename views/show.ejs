<% include ./partials/header.ejs  %>
 
<div class="w-full min-h-screen text-white bg-zinc-800 px-10 py-8">
    <div class="cards flex flex-wrap gap-10">
      <% user.posts.forEach(function(elem){ %>
        <div class="w-32 card">
            <div class=" w-full h-40 rounded-lg overflow-hidden ">
                <img class="w-full h-full object-cover" src="<%=elem.imageUrl%>" alt="">
            </div>
            <h5 class="mt-2 text-sm"><%= elem.title %></h5>
        </div>
       <%})%> 
    </div>
</div>

<% include ./partials/footer.ejs  %>